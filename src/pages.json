{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/homePage",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/my/myPage",
      "style": {
        "navigationBarTitleText": "myPage"
      }
    },
    {
      "path": "pages/test/testPage",
      "style": {
        "navigationBarTitleText": "download"
      }
    },
    {
      "path": "pages/test/layoutPage",
      "style": {
        "navigationBarTitleText": "layout测试"
      }
    },
    {
      "path": "pages/test/themePage",
      "style": {
        "navigationBarTitleText": "theme测试"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
